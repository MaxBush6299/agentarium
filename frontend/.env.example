# Frontend Environment Variables
# Copy this file to .env.local and update with actual values

# API Configuration
VITE_API_URL=http://localhost:8000/api

# Azure Entra ID Authentication
VITE_AUTH_CLIENT_ID=your-app-client-id
VITE_AUTH_AUTHORITY=https://login.microsoftonline.com/your-tenant-id
VITE_AUTH_REDIRECT_URI=http://localhost:3000/auth/callback
VITE_AUTH_SCOPES=api://your-api-uri/access_as_user

# Application Configuration
VITE_APP_NAME=Agent Demo
VITE_APP_VERSION=0.1.0
VITE_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_TRACING=true
VITE_ENABLE_CHAT_EXPORT=true
VITE_ENABLE_A2A_VISUALIZATION=true

# Development Configuration
VITE_LOG_LEVEL=debug
VITE_DEV_TOOLS_ENABLED=true
